{
  "name": "generator-ecma",
  "version": "0.0.4",
  "description": "Generates ES2015 modules with all the requisite .rc files",
  "files": [
    "app"
  ],
  "main": "app/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "scripts": {
    "compile": "rm -rf app; babel --presets es2015,stage-0 --plugins transform-runtime,add-module-exports -d app/ --ignore src/templates src/; cp -R src/templates app",
    "lint": "eslint --debug src/",
    "prepublish": "npm test; npm run compile",
    "test": "npm run lint",
    "watch": "nodemon --exec 'npm run compile' --watch src"
  },
  "author": "Benjamin Goh <masotime@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.5",
    "yeoman-generator": "^0.24.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masotime/generator-ecma.git"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.8.0",
    "nodemon": "^1.11.0"
  }
}
